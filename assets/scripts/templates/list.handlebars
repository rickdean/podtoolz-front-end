<h2> Wishlist </h2>
{{#each items as |item|}}

<p class="notepad">

  <table class="table table-reflow listTable" data-id={{id}}>
      <tbody>
          <tr>
            <th>Category</th>
              <td class='row'>{{category}}</td>
          </tr>
          <tr>
            <th>Name</th>
              <td class='row'>{{name}}</td>
              </tr>
          <tr>
            <th>Price</th>
              <td class='row'>{{price}}</td>
              </tr>
          <tr>
            <th>Url</th>
              <td class='row'>{{url}}</td>
              </tr>
      </tbody>
  </table>
  <button type="button" data-id={{id}} class="btn btn-danger btn-sm delete-list-item">Delete</button>
</p>

{{/each}}
